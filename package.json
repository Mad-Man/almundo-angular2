{
  "name": "almundo-practico",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "webpack-build": "node_modules/.bin/webpack",
    "webpack-prod": "node_modules/.bin/webpack -p",
    "sass-build": "node_modules/.bin/node-sass app/main.scss -o ./dist && npm run copy-fonts && npm run copy-media",
    "sass-build-prod": "node_modules/.bin/node-sass app/main.scss -o ./dist --output-style compressed && npm run copy-fonts && npm run copy-media",
    "copy-fonts": "node_modules/.bin/ncp ./node_modules/bootstrap-sass/assets/fonts/bootstrap ./dist/fonts && npm run copy-app-icon-fonts",
    "copy-app-icon-fonts": "node_modules/.bin/ncp ./app/fonts/fontawesome-webfont.woff2 ./dist/fonts/fontawesome-webfont.woff2",
    "copy-media": "node_modules/.bin/ncp ./app/media ./dist/media",
    "gzip-bundle-js": "node_modules/.bin/gzipme -c best dist/bundle.js",
    "gzip-vendor-js": "node_modules/.bin/gzipme -c best dist/vendor.js",
    "gzip-js": "npm run gzip-bundle-js && npm run gzip-vendor-js",
    "gzip-css": "node_modules/.bin/gzipme -c best dist/main.css",
    "build-dev": "npm run webpack-build && npm run sass-build",
    "build-prod": "npm run webpack-prod && npm run sass-build-prod && npm run removeMaps && npm run gzip-js && npm run gzip-css",
    "removeMaps": "node_modules/.bin/rimraf ./dist/*.map",
    "serve": "node_modules/.bin/babel-node ./server/index.js"
  },
  "author": "Aldama Juan Ignacio",
  "license": "MIT",
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/lodash": "^4.14.55",
    "@types/node": "^7.0.8",
    "angular2-google-maps": "^0.17.0",
    "angular2-template-loader": "^0.6.2",
    "bootstrap": "^3.3.7",
    "bootstrap-loader": "^2.0.0",
    "bootstrap-sass": "^3.3.7",
    "compression": "^1.6.2",
    "concat": "^3.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.3",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "gzipme": "^0.1.0",
    "http": "0.0.0",
    "http-server": "^0.9.0",
    "jquery": "^3.2.0",
    "lite-server": "^2.3.0",
    "lodash": "^4.17.4",
    "ncp": "^2.0.0",
    "ng2-nouislider": "^1.5.2",
    "node-babel": "^0.1.2",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "nouislider": "^9.2.0",
    "path": "^0.12.7",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "scss-loader": "0.0.1",
    "style-loader": "^0.14.1",
    "ts-loader": "^2.0.2",
    "typescript": "^2.0.3",
    "uglifyjs": "^2.4.10",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "@angular/common": ">=0.5.0 <5.0.0 || >=4.0.0-beta <5.0.0",
    "@angular/compiler": ">=2.3.1 <5.0.0 || >=4.0.0-beta <5.0.0",
    "@angular/router": ">=2.3.1 <5.0.0 || >=4.0.0-beta <5.0.0",
    "@angular/core": ">=2.3.1 <5.0.0 || >=4.0.0-beta <5.0.0",
    "@angular/forms": ">=2.3.1 <5.0.0 || >=4.0.0-beta <5.0.0",
    "@angular/http": ">=2.3.1 <5.0.0 || >=4.0.0-beta <5.0.0",
    "@angular/tsc-wrapped": ">=0.5.0 <5.0.0 || >=4.0.0-beta <5.0.0",
    "@angular/platform-browser": ">=0.5.0 <5.0.0 || >=4.0.0-beta <5.0.0",
    "@angular/platform-browser-dynamic": ">=0.5.0 <5.0.0 || >=4.0.0-beta <5.0.0",
    "@angular/animations": "^4.0.0",
    "es6-shim": "^0.35.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.1",
    "zone.js": "^0.7.2"
  }
